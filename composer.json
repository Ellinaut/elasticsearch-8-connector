{
  "name": "ellinaut/elasticsearch-8-connector",
  "description": "An abstract php connector for elasticsearch 8.x",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Marien",
      "email": "p.marien@nxiglobal.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Ellinaut\\ElasticsearchConnector\\": "src"
